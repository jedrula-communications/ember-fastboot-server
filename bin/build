#!/bin/bash
echo "Doing build script..."
echo "downloading master..."
wget https://github.com/jedrula/ember-fastboot-server/archive/master.zip
echo "unziping master..."
unzip master.zip
echo "removing master.zip..."
rm master.zip
echo "entering unzipped project dir..."
cd ember-fastboot-server-master
echo "installing npm packages..."
npm i
